<h2>{{model.title}} Listings</h2>
<div class="well">
<ul>

  {{#each model.listings as |listing|}}
     <li>{{#link-to 'listing' listing.id}}{{format-date listing.date_added}}    {{listing.title}}-${{listing.price}}{{/link-to}}</li>
  {{/each}}

</ul>
</div>
{{new-listing save="save" category=model }}
{{outlet}}
